{
  "name": "Picnic Bootstrap DevContainer",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "NEXUS_USERNAME": "${localEnv:NEXUS_USERNAME}",
      "NEXUS_PASSWORD": "${localEnv:NEXUS_PASSWORD}"
    }
  },
  "remoteEnv": {
    "NEXUS_USERNAME": "${localEnv:NEXUS_USERNAME}",
    "NEXUS_PASSWORD": "${localEnv:NEXUS_PASSWORD}",
  },
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python"]
    }
  }
}